[global_config]
[keybindings]
  split_horiz = <Primary><Alt>d
  split_vert = <Primary><Shift>d
  close_term = <Primary>w
  copy = <Primary>c
  paste = <Primary>v
  close_window = <Primary>q
[profiles]
  [[default]]
    cursor_color = "#aaaaaa"
    font = DejaVu Sans Mono Bold 11
    scrollback_infinite = True
    use_system_font = False
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
[plugins]
